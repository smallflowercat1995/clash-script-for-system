{
   "background": {
      "scripts": [ "js/common.js", "js/fetch-api.js", "js/wallabag-api.js", "js/background.js" ]
   },
   "browser_action": {
      "default_icon": "img/wallabag-icon-48a.png",
      "default_popup": "popup.html",
      "default_title": "Wallabagger"
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "default": "Alt+W",
            "mac": "Alt+W",
            "windows": "Alt+W"
         }
      },
      "wallabag-it": {
         "description": "__MSG_Wallabag_it_description__",
         "suggested_key": {
            "default": "Alt+Shift+W",
            "mac": "Alt+Shift+W",
            "windows": "Alt+Shift+W"
         }
      }
   },
   "default_locale": "en",
   "description": "__MSG_Extension_description__",
   "icons": {
      "128": "img/wallabag-icon-128.png",
      "16": "img/wallabag-icon-48a.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Wallabagger",
   "optional_permissions": [ "*://*/api/*" ],
   "options_ui": {
      "open_in_tab": true,
      "page": "options.html"
   },
   "permissions": [ "tabs", "storage", "contextMenus", "activeTab" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.10.4"
}
