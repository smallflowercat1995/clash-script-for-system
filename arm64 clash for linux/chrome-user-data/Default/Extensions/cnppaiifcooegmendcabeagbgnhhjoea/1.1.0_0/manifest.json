{
   "background": {
      "scripts": [ "js/background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "128": "assets/icon-128.png",
         "16": "assets/icon-16.png",
         "240": "assets/icon-240.png",
         "48": "assets/icon-48.png"
      }
   },
   "content_scripts": [ {
      "all_frames": false,
      "css": [ "css/content.css" ],
      "js": [ "js/content.js" ],
      "matches": [ "https://*.youtube.com/*", "https://*.netflix.com/*" ],
      "run_at": "document_end"
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "description": "ELL: Enjoy Learning Languages with Netflix and Youtube",
   "icons": {
      "128": "assets/icon-128.png",
      "16": "assets/icon-16.png",
      "240": "assets/icon-240.png",
      "48": "assets/icon-48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Enjoy Learning Languages",
   "permissions": [ "storage", "tts", "https://video.google.com/*", "https://clients5.google.com/*", "https://glosbe.com/*", "https://*.languagisfun.com/*", "https://cdn.paddle.com/paddle/paddle.js" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1.0",
   "web_accessible_resources": [ "pages/*", "assets/*", "css/*", "fonts/*", "data/*" ]
}
