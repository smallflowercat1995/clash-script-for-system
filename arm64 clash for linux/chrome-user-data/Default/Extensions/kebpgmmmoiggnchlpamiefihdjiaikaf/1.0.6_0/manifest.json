{
   "background": {
      "persistent": true,
      "scripts": [ "js/web_extension.js", "js/intercept_manager.js", "js/native_host_manager.js", "js/cookie_manager.js", "js/xdown_intercept_manager.js", "js/xdown_requests_manager.js", "js/xdown_extension.js", "js/xdown_utils.js", "js/xdown_misc.js", "js/xdown_bh_task.js", "js/xdown_main.js" ]
   },
   "browser_action": {
      "default_icon": {
         "16": "images/xdown_16.png",
         "19": "images/xdown_19.png",
         "24": "images/xdown_24.png",
         "32": "xdown_32.png",
         "38": "images/xdown_38.png"
      },
      "default_popup": "xdown_settings.html",
      "default_title": "XDown Manager"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "js/xdown_content.js" ],
      "matches": [ "file:///*", "http://*/*", "https://*/*" ],
      "run_at": "document_end"
   } ],
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
   "default_locale": "en",
   "description": "Internet Downloader enables you to download a desired item with an Internet Download Manager (xdown) application.",
   "homepage_url": "https://xdown.org/",
   "icons": {
      "128": "images/xdown_128.png",
      "32": "xdown_32.png",
      "48": "images/xdown_48.png"
   },
   "incognito": "split",
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "31.0.0.0",
   "name": "XDown",
   "offline_enabled": true,
   "permissions": [ "cookies", "webRequest", "webRequestBlocking", "downloads", "nativeMessaging", "\u003Call_urls>" ],
   "short_name": "XDown",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.6"
}
